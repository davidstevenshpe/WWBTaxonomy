<?xml version="1.0" encoding="UTF-8"?>
<r:acl xmlns:r="http://systinet.com/2005/05/repository"
       xmlns:xsd="http://www.w3.org/2001/XMLSchema"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
   <r:owner xsi:type="xsd:string">systinet:admin</r:owner>
   <r:ace>
      <r:principal type="group">system#registered</r:principal>
      <r:permission>write</r:permission>
   </r:ace>
   <r:ace>
      <r:principal type="group">system#everyone</r:principal>
      <r:permission>read</r:permission>
   </r:ace>
</r:acl>